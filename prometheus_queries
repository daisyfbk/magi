cpu=100 - (avg by(instance) (rate(node_cpu_seconds_total{mode="idle",instance!="localhost:9100"}[10s])) * 100)
network_w=max by(instance) (rate(node_network_transmit_bytes_total{instance!="localhost:9100"}[10s]))
network_r=max by(instance) (rate(node_network_receive_bytes_total{instance!="localhost:9100"}[10s]))
disk_r=max by(instance) (rate(node_disk_read_bytes_total{instance!="localhost:9100"}[30s]))
disk_w=max by(instance) (rate(node_disk_written_bytes_total{instance!="localhost:9100"}[30s]))
memory=avg by(instance) (100 * (1 - node_memory_MemAvailable_bytes{instance!="localhost:9100"}/node_memory_MemTotal_bytes{instance!="localhost:9100"}))
worker2cpu=sum by(mode) (rate(node_cpu_seconds_total{instance=~"(192.168.221.12(.*))"}[20s]))
